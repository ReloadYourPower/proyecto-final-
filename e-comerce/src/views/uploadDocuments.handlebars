<h1>Subir Archivos</h1>
<!-- Formulario para subir documentos -->
<h1>Subir Documentos</h1>
<form action="/profile/users/{{userId}}/documents" method="POST" enctype="multipart/form-data">
  <div class="form-group">
    <label for="identification">Identificaci√≥n:</label>
    <input type="file" name="identification" class="form-control">
  </div>
  <div class="form-group">
    <label for="proofOfAddress">Comprobante de domicilio:</label>
    <input type="file" name="proofOfAddress" class="form-control">
  </div>
  <div class="form-group">
    <label for="bankStatement">Comprobante de estado de cuenta:</label>
    <input type="file" name="bankStatement" class="form-control">
  </div>
  <button type="submit" class="btn btn-primary">Subir Documentos</button>
</form>


